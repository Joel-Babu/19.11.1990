	public static WebElement Close;

	//ADD
	@FindBy(xpath= "//*[@id='frmSearch:btnAdd']")
	public static WebElement Add;

	//LMS Trading dep
	@FindBy(xpath= "//*[@id='frmDetails:cbxTradingDept']")
	public static WebElement LMSTradingdept;

	//LMS SMS Job
	@FindBy(xpath= "//*[@id='frmDetails:cbxSMSJob']")
	public static WebElement LMSSMSJob;

	//Activity
	@FindBy(xpath= "//*[@id='frmDetails:cbxActivity']")
	public static WebElement Activity;

	//Task Description
	@FindBy(xpath= "//*[@id='frmDetails:txtTaskDesc']")
	public static WebElement TaskDescription;

	//Productivity Standard
	@FindBy(xpath= "//*[@id='frmDetails:txtProductivityStd']")
