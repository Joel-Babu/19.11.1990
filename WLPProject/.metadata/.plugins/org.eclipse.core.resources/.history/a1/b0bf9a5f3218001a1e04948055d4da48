	public static WebElement Type;
	
	//SEARCH
	@FindBy(xpath= "//*[@id='frmSearch:btnSearch']")
	public static WebElement Search;

	//Edit
	@FindBy(xpath= "//*[@id='frmSearch:ctrTaskMaintenanceTable:0:oEdit']")
	public static WebElement Edit;

	//LMSEdit
	@FindBy(xpath= "//*[@id='frmDetails:cbxNightFillTask']")
	public static WebElement NightFillJob;

	//save
	@FindBy(xpath= "//*[@id='frmDetails:btnSave']") 		
	public static WebElement Save;
	
	
	//parametersearchbutton
	@FindBy(xpath= "//input[@class='fcdLookUp']") 		
	public static WebElement parametersearchbutton;
	
	@FindBy(xpath= "//input[@id='frmDetails:txtTaskId']") 		
	public static WebElement Tasksetupid;

