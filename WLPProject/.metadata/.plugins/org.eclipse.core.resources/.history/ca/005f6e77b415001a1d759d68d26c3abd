package au.wow.main.objects.fcd;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class ParameterMaintenanceObjects {
	//Frame
		@FindBy(xpath= "//frame[@id='wowList']")
		public static WebElement frame;

		//User Management			
		@FindBy(xpath= "//*[@id='frmSearch:cbxTradingDept']")
		public static WebElement tradingDepartment;
		
		//Search
		@FindBy(xpath= "//*[@id='frmSearch:btnSearch']")
		public static WebElement search;
		
		
		
		//Add
		@FindBy(xpath="//input[@value='Add']")
		public static WebElement Add;
		
		//Selectingdepttogiveparametermaintenance
		@FindBy(xpath="//*[@id='frmDetails:cbxTradingDept']")
		public static WebElement Selectingdepttogiveparametermaintenance;
		
		//Selectingtypetogiveparametermaintenance
		@FindBy(xpath="//*[@id='frmDetails:cbxType']")
		public static WebElement Selectingtypetogiveparametermaintenance;
		
		
		
		//Edit
		@FindBy(xpath= "//*[@id='frmSearch:ctrParamTable:0:oEdit']")
		public static WebElement edit;
		
		//parameteridtextbox
		@FindBy(xpath="//input[@id='frmSearch:txtParamId']")
		public static WebElement parameteridtextbox;
		
		//Store User Edit
		@FindBy(xpath= "//*[@id='frmDetails:chkStoreStateUserView']")
		public static WebElement storeUserEdit;
		
		//SAVE
		@FindBy(xpath= "//*[@id='frmDetails:btnSave']")
		public static WebElement save;
		
		//close
		@FindBy(xpath= "//*[@id='frmDetails:btnCancel']")
		public static WebElement close;
		
		
		
		
		
}
